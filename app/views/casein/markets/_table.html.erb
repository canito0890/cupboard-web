<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Name", :name %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% markets.each do |market| %>
		<tr>
			<td><%= casein_table_cell_link market.name, casein_market_path(market) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_market_path(market), :method => :delete, :data => { :confirm => "Are you sure you want to delete this market?" }) %>
			</td>
		</tr>
	<% end %>
</table>